		"@babel/register": "^7.24.6",
		"@wdio/devtools-service": "^7.36.0",
		"@wdio/local-runner": "^7.36.0",
		"@wdio/mocha-framework": "^7.33.0",
		"@wdio/spec-reporter": "^7.33.0",
